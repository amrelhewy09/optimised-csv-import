<tr id="<%= movie.id %>">
  <td><%= movie.id %></td>
  <td><%= movie.name %></td>
  <td><%= movie.description %></td>
  <td><%= movie.year %></td>
  <td><%= movie.director.name %></td>
  <td><%= movie.filming_location %></td>
  <td><%= movie.country %></td>
  <td><%= movie.actors.map(&:name).join(', ') %></td>
</tr>
